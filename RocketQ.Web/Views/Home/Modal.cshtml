@model RocketQ__ASP.NET_MVC_Version_.Data.Models.Room
<div class="modal-wrapper">
    <div class="modal" id="">
        <h2>Delete question</h2>
        <p>Are you sure you want to delete this question ?</p>
        @using (Html.BeginForm("Delete", "Home", FormMethod.Post))
        {
            <!--<form action="#">--->
            <label class="sr-only" for-id="password">Type your password</label>
            <!--<input type="password" id="password" name="password" placeholder="Type your password">-->
            @Html.TextBoxFor(model => model.Password, "Type your password", new { id = "password", type = "password", name = "password" });
            <div class="buttons">
                <div class="button grey cancel" onclick="Modal().close()">
                    Cancel
                </div>
                <button class="red" type="submit" onclick="" value="Delete">Delete</button>
            </div>
            <!--</form>-->
        }
    </div>
</div>